{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "always",  
	"logLevel": {
      "default": "information",
      "Function": "information",
      "Host.Aggregator": "Error"
	},  
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request",
		"includedTypes" : "PageView;Trace"		
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  }
}
