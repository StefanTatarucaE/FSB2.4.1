{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 1 * * *"
    },
    {
      "type": "blob",
      "name": "errorOutput",
      "path": "billing-errors/billingupload-{datetime:yyyyMMdd}-error.log",
      "direction": "out",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "blob",
      "name": "infoOutput",
      "path": "billing-output/billingupload-{datetime:yyyyMMdd}-output.log",
      "direction": "out",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
