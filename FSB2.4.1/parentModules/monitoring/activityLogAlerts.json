      "alertScope": "core",
      "alertType": "ServiceHealth",
      "alertName": "Azure Service Health alert",
      "alertDescription": "This alert is triggered when an Azure service has known issues",
      "resourceTypesCondition": [
        {
          "field": "properties.incidentType",
          "equals": "Incident"
        }
      ]
	  },
