    {
      "type": 1,
      "content": {
        "json": "#### No Availability Sets are deployed yet"
      },
      "conditionalVisibility": {
        "parameterName": "name",
        "comparison": "isEqualTo"
      },
      "name": "text - 4"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "Resources\r\n| where type =~ 'Microsoft.Compute/availabilitySets'\r\n| extend [parameter(tagPrefix)]Managed=iif((tostring(tags) has '\"[parameter(tagPrefix)]Managed\":\"true\"'), \"Yes\", \"No\")\r\n| extend UpdateDomains = tostring(properties.platformUpdateDomainCount)\r\n| extend FaultDomains = tostring(properties.platformFaultDomainCount)\r\n| extend ProximityPlacementgroup = tostring(properties.proximityPlacementGroup.id)\r\n| extend AvailabilitySet=toupper(name)\r\n| project name, AvailabilitySet, subscriptionId, location, resourceGroup, UpdateDomains, FaultDomains, ProximityPlacementgroup, [parameter(tagPrefix)]Managed\r\n| join kind=leftouter (\r\nResources\r\n| where type == 'microsoft.compute/virtualmachines'\r\n| where properties.availabilitySet != \"\"\r\n| where split(properties.extended.instanceView.powerState.code, \"/\")[1]==\"running\"\r\n| extend AvailabilitySet=split( properties.availabilitySet.id,\"/\")[8]\r\n| extend powerstate=split(properties.extended.instanceView.powerState.code, \"/\")[1]\r\n| summarize runvms=count() by tostring(powerstate), tostring(AvailabilitySet)\r\n) on AvailabilitySet\r\n| project name, AvailabilitySet, subscriptionId, location, resourceGroup, UpdateDomains, FaultDomains, ProximityPlacementgroup, runvms, [parameter(tagPrefix)]Managed\r\n| join kind=leftouter (\r\nResources\r\n| where type == 'microsoft.compute/virtualmachines'\r\n| where properties.availabilitySet != \"\"\r\n| extend AvailabilitySet=split( properties.availabilitySet.id,\"/\")[8]\r\n| extend powerstate= split(properties.extended.instanceView.powerState.code, \"/\")[1]\r\n| summarize vms=count() by tostring(AvailabilitySet)\r\n) on AvailabilitySet\r\n| extend vms=iif(notnull(vms), tostring(vms),\"0\")\r\n| extend runvms=iif(notnull(runvms), tostring(runvms),\"0\")\r\n| extend Status = case(\r\ntostring(vms) == \"0\", \"Unknown\",\r\ntostring(runvms) == \"0\", \"Critical\",\r\ntostring(runvms) == \"1\", \"Warning\",\r\ntostring(runvms) != tostring(vms), \"Warning\", \"Healthy\")\r\n| where name in ({name})\r\n| where location in ({location})\r\n| where Status in ({Status})\r\n| where [parameter(tagPrefix)]Managed in ({[parameter(tagPrefix)]Managed})\r\n| distinct name, subscriptionId, location, resourceGroup, UpdateDomains, FaultDomains, vms, runvms, Status, [parameter(tagPrefix)]Managed\r\n\r\n\r\n\r\n\r\n",
        "size": 0,
        "title": "Overview availability sets",
        "noDataMessage": "No Availability Sets found  for selected filter(s)",
        "queryType": 1,
        "resourceType": "microsoft.resourcegraph/resources",
        "crossComponentResources": [
          "{SubscriptionPicker}"
        ],
        "gridSettings": {
          "formatters": [
            {
