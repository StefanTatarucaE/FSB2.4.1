        "query": "Resources\r\n | where type =~ 'microsoft.analysisservices/servers'\r\n | extend [parameter(tagPrefix)]Managed=iif((tostring(tags) has '\"[parameter(tagPrefix)]Managed\":\"true\"'), \"Yes\", \"No\")\r\n| extend Subscription = subscriptionId\r\n| extend Location = location\r\n| extend ResourceGroup = resourceGroup\r\n| extend Tier = sku.name\r\n| extend Capacity = sku.capacity\r\n| extend managedMode = tostring(iif(properties.managedMode == 1, \"Managed\", \"Not Managed\"))\r\n| extend QueryPoolConnectionMode = tostring(properties.querypoolConnectionMode)\r\n| where Tier in ({Tier})\r\n| where [parameter(tagPrefix)]Managed in ({[parameter(tagPrefix)]Managed})\r\n| where ResourceGroup in ({ResourceGroup})\r\n| where managedMode in ({ManMode})\r\n| where Location in ({Location})\r\n| project Name=id, Subscription, Location, ResourceGroup, [parameter(tagPrefix)]Managed, Tier, Capacity, managedMode, QueryPoolConnectionMode",
        "size": 0,
        "noDataMessage": "No Azure Analysis Services available with selected filters",
