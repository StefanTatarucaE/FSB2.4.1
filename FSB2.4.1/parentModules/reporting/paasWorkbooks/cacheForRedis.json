                    "id": "2a6e9715-65ac-4e99-8087-36d38c6df94b",
                    "version": "KqlParameterItem/1.0",
                    "name": "resource",
                    "label": "Redis account",
                    "type": 5,
                    "isRequired": true,
                    "isGlobal": true,
                    "multiSelect": true,
                    "quote": "'",
                    "delimiter": ",",
                    "query": "resources\r\n| where type == \"microsoft.cache/redis\"\r\n| where resourceGroup in ({resourceGroupPicker})\r\n| project id, name",
                    "crossComponentResources": [
                      "value::all"
                    ],
