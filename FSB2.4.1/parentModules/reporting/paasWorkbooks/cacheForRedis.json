                    "resourceType": "microsoft.resourcegraph/resources"
                  },
                  {
                    "id": "ca5d90b1-6a4f-4e96-8835-8b516a7d9773",
                    "version": "KqlParameterItem/1.0",
                    "name": "[parameter(tagPrefix)]Managed",
                    "label": "Managed by [parameter(companyName)]",
                    "type": 2,
                    "isRequired": true,
                    "multiSelect": true,
                    "quote": "'",
                    "delimiter": ",",
                    "value": [
                      "value::all"
                    ],
                    "typeSettings": {
                      "additionalResourceOptions": [
                        "value::all"
                      ],
                      "showDefault": false
                    },
                    "jsonData": "[\r\n    { \"value\":\"yes\", \"label\":\"yes\" },\r\n    { \"value\":\"no\", \"label\":\"no\" }\r\n]"
                  }
                ],
                "style": "above",
                "queryType": 1,
                "resourceType": "microsoft.resourcegraph/resources"
              },
              "name": "parameters - 1"
            },
            {
              "type": 3,
              "content": {
                "version": "KqlItem/1.0",
                "query": "resources\r\n| where type == \"microsoft.cache/redis\"\r\n| where id in~ ({resource})\r\n| where subscriptionId in~ ({subscriptionPicker})\r\n| where resourceGroup in~ ({resourceGroupPicker})\r\n| extend rgName = split(id, \"/\", 4)[0]\r\n| extend ['Resource group'] = strcat('/subscriptions/', subscriptionId, '/resourceGroups/', rgName)\r\n| extend redisSKU = tostring(properties.sku.name)\r\n| extend status = tolower(properties.provisioningState)\r\n| extend statusLoc = case(\r\nstatus=='succeeded',                'Running',\r\nstatus=='canceled',                 'Canceled',\r\nstatus=='creating',                 'Creating',\r\nstatus=='deleting',                 'Deleting',\r\nstatus=='scaling',                  'Scaling',\r\nstatus=='scalefailed',              'Scale failed',\r\nstatus=='recoveringscalefailure',   'Scale failed - Recovering',\r\nstatus=='provisioning',             'Provisioning',\r\nstatus=='unprovisioning',           'Decomissioning',\r\nstatus=='faulted',                  'Failed',\r\nstatus=='failed',                   'Failed',\r\nstatus=='stopped',                  'Stopped',\r\nstatus=='disabled',                 'Disabled',\r\nstatus=='updating',                 'Updating',\r\nstatus=='linking',                  'Linking',\r\nstatus)\r\n| extend Subscription = strcat('/subscriptions/', subscriptionId)\r\n| extend [parameter(tagPrefix)]Managed = iff((tostring(tags) has '\"[parameter(tagPrefix)]Managed\":\"true\"'), \"yes\", \"no\")\r\n| where [parameter(tagPrefix)]Managed in~ ({[parameter(tagPrefix)]Managed})\r\n| extend ['Replicas'] = iif(isnull(properties.replicasPerMaster), 'N\\\\A',properties.replicasPerMaster)\r\n| extend ['Redis version'] =  tostring(properties.redisVersion)\r\n| extend ['Backup RDB'] = iff(properties.redisConfiguration.['rdb-backup-enabled']==true,\"yes\", \"no\")\r\n| extend ['Backup AOF'] = iff(properties.redisConfiguration.['aof-backup-enabled']==true, \"yes\",\"no\")\r\n| project ['Resource'] = id, ['Name'] = name, ['SKU'] = redisSKU, ['Status']=statusLoc,\r\n['Location'] = location, ['Redis version'],  ['Replicas'], ['Backup RDB'], ['Backup AOF'], [parameter(tagPrefix)]Managed, ['Resource group'], Subscription",
                "size": 1,
                "title": "Azure cache for Redis accounts",
                "queryType": 1,
                "resourceType": "microsoft.resourcegraph/resources",
                "crossComponentResources": [
                  "value::all"
                ]
              },
              "conditionalVisibility": {
                "parameterName": "resource",
                "comparison": "isNotEqualTo"
              },
              "name": "resourcelist"
            },
