            {
              "type": 3,
              "content": {
                "version": "KqlItem/1.0",
                "query": "resources\r\n| where type == \"microsoft.documentdb/databaseaccounts\"\r\n| where id in~ ({resource})\r\n| where subscriptionId in~ ({subscriptionPicker})\r\n| where resourceGroup in~ ({resourceGroupPicker})\r\n| extend rgName = split(id, \"/\", 4)[0]\r\n| extend ['Resource group'] = strcat('/subscriptions/', subscriptionId, '/resourceGroups/', rgName)\r\n| extend Subscription = strcat('/subscriptions/', subscriptionId)\r\n| extend [parameter(tagPrefix)]Managed = iif((tostring(tags) has '\"[parameter(tagPrefix)]Managed\":\"true\"'), \"yes\", \"no\")\r\n| where [parameter(tagPrefix)]Managed in~ ({[parameter(tagPrefix)]Managed})\r\n| extend ['Backup Type'] =  strcat(tostring(properties.backupPolicy.type),' ( ', tostring(properties.backupPolicy.periodicModeProperties.backupStorageRedundancy), ' )')\r\n| extend ['API type enabled'] = tostring(properties.EnabledApiTypes)\r\n| extend status = case\r\n(\r\n    tolower(properties.provisioningState) == 'succeeded', 'Online',\r\n    tolower(properties.provisioningState) == 'initializing', 'Creating',\r\n    tolower(properties.provisioningState) == 'offline', 'Offline',\r\n    tolower(properties.provisioningState) == 'restoring', 'Restoring',\r\n    tolower(properties.provisioningState) == 'deleting', 'Deleting',\r\n    tolower(properties.provisioningState) == 'failed', 'Failed',\r\n    tolower(properties.provisioningState) == 'migrating', 'Migrating',\r\n    tolower(properties.provisioningState) == 'nocontent', 'No content',\r\n    tolower(properties.provisioningState) == 'updating', 'Updating',\r\n    properties.provisioningState\r\n)\r\n| project ['Resource'] = id, ['Name'] = name, Status=status, ['Account kind'] = kind, ['API type enabled'],\r\n['Location'] = location, ['Backup Type'], [parameter(tagPrefix)]Managed, ['Resource group'], Subscription\r\n| sort by (tolower(tostring(['Name']))) asc",
                "size": 1,
                "title": "Azure Cosmos DB accounts",
                "noDataMessage": "No resources deployed for chosen environment",
                "queryType": 1,
                "resourceType": "microsoft.resourcegraph/resources",
                "crossComponentResources": [
