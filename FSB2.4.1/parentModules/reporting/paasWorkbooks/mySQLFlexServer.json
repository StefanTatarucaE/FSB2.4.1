            {
              "type": 3,
              "content": {
                "version": "KqlItem/1.0",
                "query": "resources\r\n| where type =~ \"microsoft.dbformysql/flexibleservers\"\r\n| where id in~ ({resource})\r\n| where subscriptionId in~ ({subscriptionPicker})\r\n| where resourceGroup in~ ({resourceGroupPicker})\r\n| extend rgName = split(id, \"/\", 4)[0]\r\n| extend ['Resource group'] = strcat('/subscriptions/', subscriptionId, '/resourceGroups/', rgName)\r\n| extend Subscription = strcat('/subscriptions/', subscriptionId)\r\n| extend [parameter(tagPrefix)]Managed = iif((tostring(tags) has '\"[parameter(tagPrefix)]Managed\":\"true\"'), \"yes\", \"no\")\r\n| extend ['SKU name'] = tostring(sku.name)\r\n| extend ['SKU tier'] = tostring(sku.tier)\r\n| where [parameter(tagPrefix)]Managed in~ ({[parameter(tagPrefix)]Managed})\r\n| extend ['HA state'] = case (properties.haState==\"\", 'No data', properties.haState)\r\n| extend status = case(\r\n    properties.userVisibleState == \"Ready\" or properties.state == \"Ready\", \"Available\",\r\n    properties.userVisibleState == \"\" and properties.state <> \"\", properties.state,\r\n    properties.userVisibleState == \"\" or properties.state == \"\", \"--\",\r\n    properties.userVisibleState)\r\n| project ['Resource'] = id, ['Name'] = name, Status=status, ['MySQL version'] = tostring(properties.version),\r\n    ['SKU name'], ['SKU tier'], ['HA state'],\r\n    ['Location'] = location, [parameter(tagPrefix)]Managed, ['Resource group'], Subscription\r\n| sort by (tolower(tostring(['Name']))) asc",
                "size": 1,
                "title": "Azure databases for MySQL flexible server",
                "noDataMessage": "No resources deployed for chosen environment",
                "queryType": 1,
                "resourceType": "microsoft.resourcegraph/resources",
