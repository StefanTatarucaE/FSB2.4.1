    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "Resources\r\n| extend [parameter(tagPrefix)]Managed = case(\r\n    tostring(tags) has '\"[parameter(tagPrefix)]Managed\":\"true\"', \"Yes\",\r\n    tostring(tags) has '\"[parameter(tagPrefix)]Managed\":', \"Tag value invalid\", \r\n    \"Not Set\")\r\n| extend [parameter(tagPrefix)]Maintenance = case(\r\n    tostring(tags) has '\"[parameter(tagPrefix)]Maintenance\":\"true\"', \"Yes\",\r\n    tostring(tags) has '\" [parameter(tagPrefix)]Maintenance \":', \"Tag value invalid\", \r\n    \"Not Set\")    \r\n| where [parameter(tagPrefix)]Managed in~ ({[parameter(tagPrefix)]ManagedPicker})\r\n| where [parameter(tagPrefix)]Maintenance in~ ({[parameter(tagPrefix)]MaintenancePicker})\r\n| where type in~ ({ResourcetypePicker})\r\n| where resourceGroup in~ ({ResourceGroupPicker})\r\n| project id, name, subscriptionId, ResourceGroup = resourceGroup, ResourceType = type , [parameter(tagPrefix)]Managed, [parameter(tagPrefix)]Maintenance, Location = location, tags\r\n| order by tolower(name) asc \r\n| project-away name\r\n",
        "size": 3,
        "title": "Maintenance Resource List  - Overview",
        "noDataMessage": "No data to display. Please check filters at the top",
